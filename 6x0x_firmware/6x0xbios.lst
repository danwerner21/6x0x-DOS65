00C702  2               ;__IDE_PPIDETECT____________________________________________________________________________________
00C702  2               ;
00C702  2               ;  PROBE FOR PPI HARDWARE
00C702  2               ;____________________________________________________________________________________________________
00C702  2               ;
00C702  2               IDE_PPIDETECT:
00C702  2               ;
00C702  2               ; TEST FOR PPI EXISTENCE
00C702  2               ; WE SETUP THE PPI TO WRITE, THEN WRITE A VALUE OF ZERO
00C702  2               ; TO PORT A (DATALO), THEN READ IT BACK.  IF THE PPI IS THERE
00C702  2               ; THEN THE BUS HOLD CIRCUITRY WILL READ BACK THE ZERO. SINCE
00C702  2               ; WE ARE IN WRITE MODE, AN IDE CONTROLLER WILL NOT BE ABLE TO
00C702  2               ; INTERFERE WITH THE VALUE BEING READ.
00C702  2  20 E6 C8             JSR     SET_PPI_WR
00C705  2               ;
00C705  2  A9 00                LDA     #$00            ; VALUE ZERO
00C707  2  8D 20 E0             STA     PPIDELO         ; PUSH VALUE TO PORT
00C70A  2  AD 20 E0             LDA     PPIDELO         ; GET PORT VALUE
00C70D  2  C9 00                CMP     #$00
00C70F  2  60                   RTS                     ; AND RETURN
00C710  2               ;
00C710  2               
00C710  2               
00C710  2               ;*__IDE_READ_SECTOR___________________________________________________________________________________
00C710  2               ;*
00C710  2               ;*  READ IDE SECTOR (IN LBA) INTO BUFFER
00C710  2               ;*
00C710  2               ;*____________________________________________________________________________________________________
00C710  2               IDE_READ_SECTOR:
00C710  2  AD 12 05             LDA     debsehd         ; STORE CURRENT PARMS
00C713  2  CD 15 05             CMP     Cdebsehd        ;
00C716  2  D0 13                BNE     IDE_READ_SECTOR_DIRTY
00C718  2  AD 11 05             LDA     debcylm         ;
00C71B  2  CD 14 05             CMP     Cdebcylm        ;
00C71E  2  D0 0B                BNE     IDE_READ_SECTOR_DIRTY
00C720  2  AD 10 05             LDA     debcyll         ;
00C723  2  CD 13 05             CMP     Cdebcyll        ;
00C726  2  D0 03                BNE     IDE_READ_SECTOR_DIRTY
00C728  2  A9 00                LDA     #$00            ; ZERO = 1 ON RETURN = OPERATION OK
00C72A  2  60                   RTS
00C72B  2               
