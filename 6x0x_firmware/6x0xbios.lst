00C8CE  2               
00C8CE  2  09 20                ORA     #PPIDE_WR_LINE  ; ASSERT WRITE PIN
00C8D0  2  8D 22 E0             STA     PPIDECNTRL
00C8D3  2               
00C8D3  2  49 20                EOR     #PPIDE_WR_LINE  ; DE ASSERT WR PIN
00C8D5  2  8D 22 E0             STA     PPIDECNTRL
00C8D8  2               
00C8D8  2  A9 00                LDA     #$00
00C8DA  2  8D 22 E0             STA     PPIDECNTRL      ;DEASSERT ALL CONTROL PINS
00C8DD  2  60                   RTS
