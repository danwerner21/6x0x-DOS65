008E1B  3  00 BD 30 8E  
008E1F  3  E8 C9 24 F0  
008E36  3  20 CD 88             JSR     NEWLINE
008E39  3  48 DA 5A A2          PRTS    " IO=0x$"
008E3D  3  00 BD 52 8E  
008E41  3  E8 C9 24 F0  
008E59  3  A9 03                LDA     #>RTCIO         ; GET BASE PORT
008E5B  3  20 A1 88             JSR     PRTHEXBYTE      ; PRINT BASE PORT
008E5E  3  A9 94                LDA     #<RTCIO         ; GET BASE PORT
008E60  3  20 A1 88             JSR     PRTHEXBYTE      ; PRINT BASE PORT
008E63  3  20 CD 88             JSR     NEWLINE         ;
008E66  3  AD 75 8E             LDA     RTCVALUE        ; RESET PORT VALUE
008E69  3  8F 94 03 00          STA     f:RTCIO
