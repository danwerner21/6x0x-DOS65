008EDD  3               ;        AND     #$F7
008EDD  3               ;        STA     f:RTCIO
008EDD  3               ;:
008EDD  3               ;        DEY
008EDD  3               ;        CPY     #$0000
008EDD  3               ;        BNE     RTC_BEEP1
008EDD  3               ;
008EDD  3               ;        INDEX8
008EDD  3               ;        LDA     RTCVALUE
