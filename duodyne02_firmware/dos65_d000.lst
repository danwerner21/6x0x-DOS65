008DF9  3  8D 8A 03             STA     PPIDECNTRL
008DFC  3               
008DFC  3  49 20                EOR     #PPIDE_WR_LINE  ; DE ASSERT WR PIN
008DFE  3  8D 8A 03             STA     PPIDECNTRL
008E01  3               
008E01  3  A9 00                LDA     #$00
008E03  3  8D 8A 03             STA     PPIDECNTRL      ;DEASSERT ALL CONTROL PINS
