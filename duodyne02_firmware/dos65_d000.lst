008DCF  3               IDE_READ:
008DCF  3  20 07 8E             JSR     SET_PPI_RD      ; SETUP FOR A READ CYCLE
008DD2  3  8D 8A 03             STA     PPIDECNTRL      ;DRIVE ADDRESS ONTO CONTROL LINES
008DD5  3  09 40                ORA     #PPIDE_RD_LINE  ; ASSERT RD PIN
008DD7  3  8D 8A 03             STA     PPIDECNTRL
008DDA  3  AE 88 03             LDX     PPIDELO         ; READ LOWER BYTE
008DDD  3  AC 89 03             LDY     PPIDEHI         ; READ UPPER BYTE
008DE0  3  49 40                EOR     #PPIDE_RD_LINE  ; DE-ASSERT RD SIGNAL
008DE2  3  8D 8A 03             STA     PPIDECNTRL
008DE5  3  A9 00                LDA     #$00
008DE7  3  8D 8A 03             STA     PPIDECNTRL      ;DEASSERT ALL CONTROL PINS
008DEA  3  60                   RTS
