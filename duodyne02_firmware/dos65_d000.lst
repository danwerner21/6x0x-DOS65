008E17  3               ;__RTC DRIVERS____________________________________________________________________________________________________________________
008E17  3               ;
008E17  3               ; 	Duodyne Real Time Clock drivers (ROMRAM Card)
008E17  3               ;
008E17  3               ;	Entry points:
008E17  3               ;		RTC_INIT        - called during OS init
008E17  3               ;		RTC_READ        - read a sector from drive
008E17  3               ;		RTC_WRITE       - write a sector to drive
008E17  3               ;		RTC_LED         - UPDATE LEDS
008E17  3               ;               RTC_BUTTON      - GET BUTTON STATUS
008E17  3               ;               RTC_BEEP        - BEEP RTC SPEAKER
008E17  3               ;
008E17  3               ;________________________________________________________________________________________________________________________________
008E17  3               ;
008E17  3               ; RAM BANK $1E is RAM area for Drivers
008E17  3               ; RAM BANK $1D is operating bank for DOS/65 $8000-$FFFF
008E17  3               ;
008E17  3               ; ROM BANKS $00 and $0C-$0F are reserved for ROMWBW code
008E17  3               ; ROM Drive starts in bank $22
008E17  3               ; RAM Drive starts in bank $02
008E17  3               ;
008E17  3               ;
008E17  3               ;
008E17  3               RTCIO           = IO+$94        ; PORT A
008E17  3               ;
008E17  3               ; Write Bits
008E17  3               ; Latch IO   MSB   7 6 5 4 3 2 1 0  LSB
008E17  3               ;                  | | | | | | | |-- USER LED 0
008E17  3               ;                  | | | | | | |---- USER LED 1
008E17  3               ;                  | | | | | |------ SPEAKER
008E17  3               ;                  | | | | |-------- /MEMORY ENABLE
008E17  3               ;                  | | | |---------- RTC RESET
008E17  3               ;                  | | |------------ RTC WRITE
008E17  3               ;                  | |-------------- RTC CLK
008E17  3               ;                  |---------------- RTC DATA
008E17  3               ;
008E17  3               ; Read Bits
008E17  3               ; Latch IO   MSB   7 6 5 4 3 2 1 0  LSB
008E17  3               ;                  X | X X X X | |-- RTC DQ
008E17  3               ;                    |         |---- /MEMORY ENABLE
008E17  3               ;                    |-------------- USER BUTTON
008E17  3               ;
008E17  3               ;
008E17  3               ;
008E17  3               
008E17  3               
008E17  3               mask_data_out   = $01           ; RTC data line
008E17  3               mask_data_in    = $80           ; RTC data line
008E17  3               mask_clk        = $40           ; RTC Serial Clock line
008E17  3               mask_wrt_en     = $20           ; RTC Serial Clock line
008E17  3               mask_ce         = $10           ; RTC CHIP ENABLE
008E17  3               
008E17  3               mask_mem_en     = $08
008E17  3               mask_spk        = $04
008E17  3               mask_led1       = $02
008E17  3               mask_led0       = $01
008E17  3               
008E17  3               mask_button     = $40
